if(purpose == "HBW") {
    intercept = 1;
    timeParam = -1;
    impedanceParam = 1;
    attractionParam = 1;


    travelImpedance = Math.exp(timeParam * travelTime);
    impedanceImpact = travelImpedance * impedanceParam;
    attraction = Math.log(totalEmployees);
    attractionImpact = attraction * attractionParam;

    utility = 0.;
    if(log) {
        print("_______________________________________________")
        print("Calculating Utility: base zone " + baseZone + " | target zone " + targetZone + " | purpose " + purpose);

        print("Intercept            = " + intercept);
        utility += intercept;
        print("Utility              = " + utility);

        print("***");
        print("Impedance            = Math.exp(timeParam * travelTime)");
        print("Impedance            = Math.exp("+ timeParam + " * " + travelTime + ")");
        print("Impedance            = Math.exp(" + (timeParam * travelTime) + ")");
        print("Impedance            = " + travelImpedance);
        print("Impedance Impact     = impedance * impedanceParam");
        print("Impedance Impact     = " + travelImpedance + " * " + impedanceParam);
        print("Impedance Impact     = " + impedanceImpact);
        utility += impedanceImpact;
        print("Utility              = " + utility);

        print("***");
        print("Attraction           = Math.log(totalEmployees)");
        print("Attraction           = Math.log(" + totalEmployees + ")");
        print("Attraction           = " + attraction);
        print("Attraction Impact    = attraction * attractionParam");
        print("Attraction Impact    = " + attraction + " * " + attractionParam + ")");
        print("Attraction Impact    = " + attractionImpact);
        utility += attractionImpact;
        print("Utility              = " + utility);

        print("_______________________________________________")

        utility;
    } else {
        utility = intercept + impedanceImpact + attractionImpact;
    }
} else if(purpose == "HBE") {
    intercept = 1;
    timeParam = -1;
    impedanceParam = 1;
    attractionParam = 1;


    travelImpedance = Math.exp(timeParam * travelTime);
    impedanceImpact = travelImpedance * impedanceParam;
    attraction = Math.log(schoolEnrollment);
    attractionImpact = attraction * attractionParam;
    utility = 0.;
    if(log) {
        print("_______________________________________________")
        print("Calculating Utility: base zone " + baseZone + " | target zone " + targetZone + " | purpose " + purpose);

        print("Intercept            = " + intercept);
        utility += intercept;
        print("Utility              = " + utility);

        print("***");
        print("Impedance            = Math.exp(timeParam * travelTime)");
        print("Impedance            = Math.exp("+ timeParam + " * " + travelTime + ")");
        print("Impedance            = Math.exp(" + (timeParam * travelTime) + ")");
        print("Impedance            = " + travelImpedance);
        print("Impedance Impact     = impedance * impedanceParam");
        print("Impedance Impact     = " + travelImpedance + " * " + impedanceParam);
        print("Impedance Impact     = " + impedanceImpact);
        utility += impedanceImpact;
        print("Utility              = " + utility);

        print("***");
        print("Attraction           = Math.log(schoolEnrollment)");
        print("Attraction           = Math.log(" + totalEmployees + ")");
        print("Attraction           = " + attraction);
        print("Attraction Impact    = attraction * attractionParam");
        print("Attraction Impact    = " + attraction + " * " + attractionParam + ")");
        print("Attraction Impact    = " + attractionImpact);
        utility += attractionImpact;
        print("Utility              = " + utility);

        print("_______________________________________________")

        utility;

    } else {
          utility = intercept + impedanceImpact + attractionImpact;
    }
} else if(purpose == "HBS") {
    intercept = 1;
    timeParam = -1;
    impedanceParam = 1;
    attractionParam = 1;
    budgetOffsetParam = -1;


    travelImpedance = Math.exp(timeParam * travelTime);
    impedanceImpact = travelImpedance * impedanceParam;
    attraction = Math.log(retailEmployees);
    attractionImpact = attraction * attractionParam;

    budgetOffset = Math.abs(budget - travelTime);
    budgetOffsetImpact = budgetOffset * budgetOffsetParam;


    utility = 0.;

    if(log) {
        print("_______________________________________________")
        print("Calculating Utility: base zone " + baseZone + " | target zone " + targetZone + " | purpose " + purpose);

        print("Intercept            = " + intercept);
        utility += intercept;
        print("Utility              = " + utility);

        print("***");
        print("Impedance            = Math.exp(timeParam * travelTime)");
        print("Impedance            = Math.exp("+ timeParam + " * " + travelTime + ")");
        print("Impedance            = Math.exp(" + (timeParam * travelTime) + ")");
        print("Impedance            = " + travelImpedance);
        print("Impedance Impact     = impedance * impedanceParam");
        print("Impedance Impact     = " + travelImpedance + " * " + impedanceParam);
        print("Impedance Impact     = " + impedanceImpact);
        utility += impedanceImpact;
        print("Utility              = " + utility);

        print("***");
        print("Attraction           = Math.log(retailEmployees)");
        print("Attraction           = Math.log(" + retailEmployees + ")");
        print("Attraction           = " + attraction);
        print("Attraction Impact    = attraction * attractionParam");
        print("Attraction Impact    = " + attraction + " * " + attractionParam + ")");
        print("Attraction Impact    = " + attractionImpact);
        utility += attractionImpact;
        print("Utility              = " + utility);

        print("***");
        print("Budget Offset        = Math.abs(BudgetPerTrip - travelTime)");
        print("Budget Offset        = Math.abs(" + budget + " - " + travelTime+")");
        print("Budget Offset        = " + budgetOffset)
        print("Budget Offset Impact = budgetOffset * budgetOffsetParam");
        print("Budget Offset Impact = " + budgetOffset * budgetOffsetParam);
        print("Budget Offset Impact = " + budgetOffsetImpact);
        utility += budgetOffsetImpact;
        print("Utility              = " + utility);

        print("_______________________________________________")

        utility;
    } else {
        utility = intercept + impedanceImpact + attractionImpact + budgetOffsetImpact;
    }
} else if(purpose == "HBO") {
    intercept = 1;
    timeParam = -1;
    impedanceParam = 1;
    attractionParam = 1;
    budgetOffsetParam = -1;


    travelImpedance = Math.exp(timeParam * travelTime);
    impedanceImpact = travelImpedance * impedanceParam;
    attraction = Math.log(households) + Math.log(otherEmployees);
    attractionImpact = attraction * attractionParam;

    budgetOffset = Math.abs(budget - travelTime);
    budgetOffsetImpact = budgetOffset * budgetOffsetParam;


    utility = 0.;

    if(log) {
        print("_______________________________________________")
        print("Calculating Utility: base zone " + baseZone + " | target zone " + targetZone + " | purpose " + purpose);

        print("Intercept            = " + intercept);
        utility += intercept;
        print("Utility              = " + utility);

        print("***");
        print("Impedance            = Math.exp(timeParam * travelTime)");
        print("Impedance            = Math.exp("+ timeParam + " * " + travelTime + ")");
        print("Impedance            = Math.exp(" + (timeParam * travelTime) + ")");
        print("Impedance            = " + travelImpedance);
        print("Impedance Impact     = impedance * impedanceParam");
        print("Impedance Impact     = " + travelImpedance + " * " + impedanceParam);
        print("Impedance Impact     = " + impedanceImpact);
        utility += impedanceImpact;
        print("Utility              = " + utility);

        print("***");
        print("Attraction           = Math.log(# of households) + Math.log(otherEmployees)");
        print("Attraction           = Math.log(" + households + ") + Math.log(" + otherEmployees +")");
        print("Attraction           = " + attraction);
        print("Attraction Impact    = attraction * attractionParam");
        print("Attraction Impact    = " + attraction + " * " + attractionParam + ")");
        print("Attraction Impact    = " + attractionImpact);
        utility += attractionImpact;
        print("Utility              = " + utility);

        print("***");
        print("Budget Offset        = Math.abs(BudgetPerTrip - travelTime)");
        print("Budget Offset        = Math.abs(" + budget + " - " + travelTime+")");
        print("Budget Offset        = " + budgetOffset)
        print("Budget Offset Impact = budgetOffset * budgetOffsetParam");
        print("Budget Offset Impact = " + budgetOffset * budgetOffsetParam);
        print("Budget Offset Impact = " + budgetOffsetImpact);
        utility += budgetOffsetImpact;
        print("Utility              = " + utility);

        print("_______________________________________________")

        utility;
    } else {
        utility = intercept + impedanceImpact + attractionImpact + budgetOffsetImpact;
    }
} else {
    0.;
}
