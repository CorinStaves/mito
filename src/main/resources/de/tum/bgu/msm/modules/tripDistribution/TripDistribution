travelDistanceParamHbs = -0.045;
impedanceParamHbs = 14.5;

travelDistanceParamHbo = -0.062;
impedanceParamHbo = 53;

travelDistanceParamHbw = -0.07;
impedanceParamHbw = 9;

travelDistanceParamHbe = -0.149;
impedanceParamHbe = 28.3;

travelDistanceParamNhbw = -0.03;
impedanceParamNhbw = 16.1;

travelDistanceParamNhbo = -0.09;
impedanceParamNhbo = 16.6;

var calculateHBW = function (travelDistance, attraction) {
    if(attraction == 0) {
        return 0.;
    }
    impedance = impedanceParamHbw * Math.exp(travelDistanceParamHbw * travelDistance);
    return Math.exp(impedance) * attraction;
}

var calculateHBE = function (travelDistance, attraction) {
    if(attraction == 0) {
        return 0.;
    }
    impedance = impedanceParamHbe * Math.exp(travelDistanceParamHbe * travelDistance);
    return Math.exp(impedance) * attraction;
}

var calculateHBS  = function (travelDistance, attraction) {
    if(attraction == 0) {
            return 0.;
    }
    impedance = impedanceParamHbs * Math.exp(travelDistanceParamHbs * travelDistance);
    return Math.exp(impedance) * attraction;
}

var calculateHBO = function (travelDistance, attraction) {
    if(attraction == 0) {
        return 0.;
    }
    impedance = impedanceParamHbo * Math.exp(travelDistanceParamHbo * travelDistance);
    return Math.exp(impedance) * attraction;
}

var calculateNHBW = function (travelDistance, attraction) {
    if(attraction == 0) {
        return 0.;
    }
    impedance = impedanceParamNhbw * Math.exp(travelDistanceParamNhbw * travelDistance);
    return Math.exp(impedance) * attraction;
}

var calculateNHBO = function (travelDistance, attraction) {
    if(attraction == 0) {
        return 0.;
    }
    impedance = impedanceParamNhbo * Math.exp(travelDistanceParamNhbo * travelDistance);
    return Math.exp(impedance) * attraction;
}

